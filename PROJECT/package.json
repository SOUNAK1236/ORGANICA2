{
  "name": "organic-food-traceability",
  "version": "1.0.0",
  "description": "Blockchain-based organic food traceability system",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:blockchain && npm run install:backend && npm run install:frontend",
    "install:blockchain": "cd blockchain && npm install",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "blockchain:compile": "cd blockchain && npx truffle compile",
    "blockchain:migrate": "cd blockchain && npx truffle migrate",
    "blockchain:test": "cd blockchain && npx truffle test",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\""
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "organic",
    "food",
    "traceability",
    "supply-chain"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}